  "devDependencies": {
    "@angular/core": "~2.0.0-rc.6",

    "typescript": "2.0.0",
    "typings": "1.3.3",

    "core-js": "^2.4.0",
    "rxjs": "5.0.0-beta.11",
    "zone.js": "~0.6.13",

    "gulp": "3.9.0",
    "i18next-parser": "0.7.0"
